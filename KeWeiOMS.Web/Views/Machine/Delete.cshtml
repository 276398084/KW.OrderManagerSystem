@model KeWeiOMS.Domain.MachineType

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>KeWeiOMS.Domain.MachineType</title>
</head>
<body>
<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>KeWeiOMS.Domain.MachineType</legend>
    <div class="display-label">主键</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Id)
    </div>

    <div class="display-label">编号</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Code)
    </div>

    <div class="display-label">名称</div>
