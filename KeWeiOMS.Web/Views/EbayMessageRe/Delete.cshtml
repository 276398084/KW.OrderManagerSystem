@model KeWeiOMS.Domain.EbayMessageReType

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>KeWeiOMS.Domain.EbayMessageReType</title>
</head>
<body>
<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>KeWeiOMS.Domain.EbayMessageReType</legend>
    <div class="display-label">主键</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Id)
    </div>

    <div class="display-label">回复消息Id</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.MessageId)
    </div>

    <div class="display-label">回复主题</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.SubjectRe)
    </div>

    <div class="display-label">回复内容</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.BodyRe)
    </div>

    <div class="display-label">回复时间</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ReplayOn)
    </div>

    <div class="display-label">回复人</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ReplayBy)
    </div>


</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("返回列表", "Index")
    </p>
}


</body>
</html>
