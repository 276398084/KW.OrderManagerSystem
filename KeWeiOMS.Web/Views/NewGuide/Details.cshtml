@model KeWeiOMS.Domain.NewGuideType

@{
   Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>KeWeiOMS.Domain.NewGuideType</title>
</head>
<body>
<fieldset>
    <legend>KeWeiOMS.Domain.NewGuideType</legend>
    <div class="display-label">主键</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Id)
    </div>

    <div class="display-label">新SKU</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.NewSku)
    </div>

    <div class="display-label">旧SKU</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.OldSku)
    </div>

    <div class="display-label">标题</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Title)
    </div>

    <div class="display-label">图片</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Pic)
    </div>

    <div class="display-label">网址</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Url)
    </div>

    <div class="display-label">价格</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Price)
    </div>

    <div class="display-label">规格</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ColorSize)
    </div>

    <div class="display-label">备注</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Remark)
    </div>

    <div class="display-label">状态</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Status)
    </div>

    <div class="display-label">采购标记</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.IsCheck)
    </div>

    <div class="display-label">创建时间</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.CreateOn)
    </div>


</fieldset>
<p>
    @Html.ActionLink("编辑", "Edit", new { id=Model.Id }) |
    @Html.ActionLink("返回列表", "Index")
</p>
</body>
</html>
