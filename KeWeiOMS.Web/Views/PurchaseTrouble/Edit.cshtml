        $('#e_BuyOn').datebox();


        $('#e_ReceiveOn').datebox();

        $('#e_CreateBy').combobox({
          url: 'User/UserSrc',
          editable: true
      });


        $('#e_CreateOn').datebox();

        $('#e_DealBy').combobox({
          url: 'User/UserSrc',
          editable: true
      });


        $('#e_DealOn').datebox();


    });
</script>
