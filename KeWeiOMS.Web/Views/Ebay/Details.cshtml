@model KeWeiOMS.Domain.EbayType

@{
   Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>KeWeiOMS.Domain.EbayType</title>
</head>
<body>
<fieldset>
    <legend>KeWeiOMS.Domain.EbayType</legend>
    <div class="display-label">主键</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Id)
    </div>

    <div class="display-label">编号</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ItemId)
    </div>

    <div class="display-label">标题</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ItemTitle)
    </div>

    <div class="display-label">货币</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Currency)
    </div>

    <div class="display-label">价格</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Price)
    </div>

    <div class="display-label">图片链接</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.PicUrl)
    </div>

    <div class="display-label">初始数量</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.StartNum)
    </div>

    <div class="display-label">现在数量</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.NowNum)
    </div>

    <div class="display-label">产品链接</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ProductUrl)
    </div>

    <div class="display-label">开始时间</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.StartTime)
    </div>

    <div class="display-label">同步时间</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.CreateOn)
    </div>

    <div class="display-label">账号</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Account)
    </div>


</fieldset>
<p>
    @Html.ActionLink("编辑", "Edit", new { id=Model.Id }) |
    @Html.ActionLink("返回列表", "Index")
</p>
</body>
</html>
