@model KeWeiOMS.Domain.SoresAddType

@{
   Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>KeWeiOMS.Domain.SoresAddType</title>
</head>
<body>
<fieldset>
    <legend>KeWeiOMS.Domain.SoresAddType</legend>
    <div class="display-label">主键</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Id)
    </div>

    <div class="display-label">人员</div>
