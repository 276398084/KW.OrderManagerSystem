@model KeWeiOMS.Domain.OrderType

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>KeWeiOMS.Domain.OrderType</title>
</head>
<body>
<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>KeWeiOMS.Domain.OrderType</legend>
    <div class="display-label">主键</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Id)
    </div>

    <div class="display-label">订单编号</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.OrderNo)
    </div>

    <div class="display-label">外部编号</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.OrderExNo)
    </div>

    <div class="display-label">订单状态</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Status)
    </div>

    <div class="display-label">合并订单</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.IsMerger)
    </div>

    <div class="display-label">拆分订单</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.IsSplit)
    </div>

    <div class="display-label">缺货订单</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.IsOutOfStock)
    </div>

    <div class="display-label">重发订单</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.IsRepeat)
    </div>

    <div class="display-label">货币</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.CurrencyCode)
    </div>

    <div class="display-label">总金额</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Amount)
    </div>

    <div class="display-label">流水交易号</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.TId)
    </div>

    <div class="display-label">买家</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.BuyerName)
    </div>

    <div class="display-label">买家邮箱</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.BuyerEmail)
    </div>

    <div class="display-label">买家Id</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.BuyerId)
    </div>

    <div class="display-label">买家留言</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.BuyerMemo)
    </div>

    <div class="display-label">商家留言</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.SellerMemo)
    </div>

    <div class="display-label">包裹截留留言</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.CutOffMemo)
    </div>

    <div class="display-label">发货方式</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.LogisticMode)
    </div>

    <div class="display-label">国家</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Country)
    </div>

    <div class="display-label">地址Id</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.AddressId)
    </div>

    <div class="display-label">总量</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Weight)
    </div>

    <div class="display-label">运费</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Freight)
    </div>

    <div class="display-label">生成时间</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.GenerateOn)
    </div>

    <div class="display-label">同步时间</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.CreateOn)
    </div>

    <div class="display-label">扫描时间</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ScanningOn)
    </div>

    <div class="display-label">扫描人</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ScanningBy)
    </div>


</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("返回列表", "Index")
    </p>
}


</body>
</html>
