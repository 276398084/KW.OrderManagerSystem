        @Html.DisplayFor(model => model.ModifiedOn)
    </div>

    <div class="display-label">修改用户主键</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ModifiedUserId)
    </div>

    <div class="display-label">修改用户</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ModifiedBy)
    </div>


