@model KeWeiOMS.Domain.CurrencyType
@{
    Layout = null;
}
<script type="text/javascript">
    $(function () {
        //这里写加载下拉列表、修改编辑控件的初始化值
	
        $('#e_CreateOn').datebox();


    });
</script>
@using (Html.BeginForm("Edit", "Currency", FormMethod.Post, new { id = "e_form" }))
{
<fieldset>
<table class="editForm">
	<tr>
		<td>
            @Html.LabelFor(model => model.Id,"主键表:")
		</td>
		<td>
            @Html.TextBoxFor(model => model.Id,new{@id="e_Id"})
		</td>
	</tr>

	<tr>
		<td>
            @Html.LabelFor(model => model.CurrencyName,"货币:")
		</td>
		<td>
            @Html.TextBoxFor(model => model.CurrencyName,new{@id="e_CurrencyName"})
		</td>
	</tr>

	<tr>
		<td>
            @Html.LabelFor(model => model.CurrencySign,"符号:")
		</td>
		<td>
            @Html.TextBoxFor(model => model.CurrencySign,new{@id="e_CurrencySign"})
		</td>
	</tr>

	<tr>
		<td>
            @Html.LabelFor(model => model.CurrencyValue,"值:")
		</td>
		<td>
            @Html.TextBoxFor(model => model.CurrencyValue,new{@id="e_CurrencyValue"})
		</td>
	</tr>

	<tr>
		<td>
            @Html.LabelFor(model => model.CreateOn,"CreateOn:")
		</td>
		<td>
            @Html.TextBoxFor(model => model.CreateOn,new{@id="e_CreateOn"})
		</td>
	</tr>


    
</table>
</fieldset>
}
