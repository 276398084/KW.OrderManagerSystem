@model KeWeiOMS.Domain.RefundAmountType

@{
    Layout = null;
}

<script type="text/javascript">

    $(function () {

		        $('#c_AuditBy').combobox({
          url: 'User/UserSrc',
          editable: true
      });


        $('#c_AuditOn').datebox();


        $('#c_CreateOn').datebox();

        $('#c_CreateBy').combobox({
          url: 'User/UserSrc',
          editable: true
      });


    });
    
</script>

