@model KeWeiOMS.Domain.WarehouseStockType

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>KeWeiOMS.Domain.WarehouseStockType</title>
</head>
<body>
<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>KeWeiOMS.Domain.WarehouseStockType</legend>
    <div class="display-label">主键</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Id)
    </div>

    <div class="display-label">仓库ID</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.WId)
    </div>

    <div class="display-label">仓库</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Warehouse)
    </div>

    <div class="display-label">商品ID</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.PId)
    </div>

    <div class="display-label">商品SKU</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.SKU)
    </div>

    <div class="display-label">商品名称</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Title)
    </div>

    <div class="display-label">盘点数量</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Qty)
    </div>

    <div class="display-label">更新时间</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.UpdateOn)
    </div>


</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("返回列表", "Index")
    </p>
}


</body>
</html>
