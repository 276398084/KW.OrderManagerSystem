@model KeWeiOMS.Domain.MachinType

@{
   Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>KeWeiOMS.Domain.MachinType</title>
</head>
<body>
<fieldset>
    <legend>KeWeiOMS.Domain.MachinType</legend>
    <div class="display-label">主键</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Id)
    </div>

    <div class="display-label">编号</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Code)
    </div>

    <div class="display-label">名称</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Name)
    </div>

    <div class="display-label">状态</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Status)
    </div>

    <div class="display-label">使用人</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.UserName)
    </div>

    <div class="display-label">开始时间</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.StartDate)
    </div>

    <div class="display-label">结束时间</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.EndDate)
    </div>

    <div class="display-label">过去使用人</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.UserNameOld)
    </div>

    <div class="display-label">过去状态</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.StatusOld)
    </div>

    <div class="display-label">开始时</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.StartDateOld)
    </div>

    <div class="display-label">结束时</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.EndDateOld)
    </div>


</fieldset>
<p>
    @Html.ActionLink("编辑", "Edit", new { id=Model.Id }) |
    @Html.ActionLink("返回列表", "Index")
</p>
</body>
</html>
