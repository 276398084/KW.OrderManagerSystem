@model KeWeiOMS.Domain.PermissionType

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>KeWeiOMS.Domain.PermissionType</title>
</head>
<body>
<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>KeWeiOMS.Domain.PermissionType</legend>
    <div class="display-label">主键</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Id)
    </div>

    <div class="display-label">资料类别（主体）</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ResourceCategory)
    </div>

    <div class="display-label">资源主键（主体）</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ResourceId)
    </div>

    <div class="display-label">操作权限主键</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.PermissionId)
    </div>

    <div class="display-label">权限约束表达式</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.PermissionConstraint)
    </div>

    <div class="display-label">有效</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Enabled)
    </div>

    <div class="display-label">删除标记</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.DeletionStateCode)
    </div>

    <div class="display-label">备注</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Description)
    </div>

    <div class="display-label">创建日期</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.CreateOn)
    </div>

    <div class="display-label">创建用户</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.CreateBy)
    </div>

    <div class="display-label">创建用户主键</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.CreateUserId)
    </div>

    <div class="display-label">修改日期</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ModifiedOn)
    </div>

    <div class="display-label">修改用户</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ModifiedBy)
    </div>

    <div class="display-label">修改用户主键</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ModifiedUserId)
    </div>


</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("返回列表", "Index")
    </p>
}


</body>
</html>
