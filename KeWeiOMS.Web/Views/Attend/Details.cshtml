@model KeWeiOMS.Domain.AttendType

@{
   Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>KeWeiOMS.Domain.AttendType</title>
</head>
<body>
<fieldset>
    <legend>KeWeiOMS.Domain.AttendType</legend>
    <div class="display-label">主键</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Id)
    </div>

    <div class="display-label">用户Id</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.UserId)
    </div>

    <div class="display-label">用户编号</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.UserCode)
    </div>

    <div class="display-label">姓名</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.姓名)
    </div>

    <div class="display-label">日期</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.CurrentDate)
    </div>

    <div class="display-label">上午上班</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.AMStart)
    </div>

    <div class="display-label">上午下班</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.AMEnd)
    </div>

    <div class="display-label">下午上班</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.PMStart)
    </div>

    <div class="display-label">下午下班</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.PMEnd)
    </div>

    <div class="display-label">IP</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.IP)
    </div>


</fieldset>
<p>
    @Html.ActionLink("编辑", "Edit", new { id=Model.Id }) |
    @Html.ActionLink("返回列表", "Index")
</p>
</body>
</html>
