@model KeWeiOMS.Domain.PlanDaoType

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>KeWeiOMS.Domain.PlanDaoType</title>
</head>
<body>
<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>KeWeiOMS.Domain.PlanDaoType</legend>
    <div class="display-label">主键</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Id)
    </div>

    <div class="display-label">采购计划编号</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.PlanNo)
    </div>

    <div class="display-label">采购SKU</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.SKU)
    </div>

    <div class="display-label">产品名称</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Title)
    </div>

    <div class="display-label">采购时间</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.BuyOn)
    </div>

    <div class="display-label">发货时间</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.SendOn)
    </div>

    <div class="display-label">物流</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.LogisticMode)
    </div>

    <div class="display-label">追踪号</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.TrackCode)
    </div>

    <div class="display-label">计划中采购数量</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.PlanQty)
    </div>

    <div class="display-label">实际到货数量</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.RealQty)
    </div>

    <div class="display-label">供货商</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Suppliers)
    </div>

    <div class="display-label">产品金额</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Price)
    </div>

    <div class="display-label">到货状态</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Status)
    </div>

    <div class="display-label">清点人</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.CheckBy)
    </div>

    <div class="display-label">合格率</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.PassRate)
    </div>

    <div class="display-label">产品检验人</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ValiBy)
    </div>

    <div class="display-label">采购备注</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.PlanMemo)
    </div>

    <div class="display-label">到货备注</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Memo)
    </div>


</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("返回列表", "Index")
    </p>
}


</body>
</html>
