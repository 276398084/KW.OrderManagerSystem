@model KeWeiOMS.Domain.MachinesOldType
@{
    Layout = null;
}
<script type="text/javascript">
    $(function () {
        //这里写加载下拉列表、修改编辑控件的初始化值
	
        $('#e_StartDateOld').datebox();


        $('#e_EndDateOld').datebox();


    });
</script>
@using (Html.BeginForm("Edit", "MachinesOld", FormMethod.Post, new { id = "e_form" }))
{
<fieldset>
<table class="editForm">
	<tr>
		<td>
            @Html.LabelFor(model => model.Id,"主键:")
		</td>
		<td>
            @Html.TextBoxFor(model => model.Id,new{@id="e_Id"})
		</td>
	</tr>

	<tr>
		<td>
            @Html.LabelFor(model => model.MachineId,"设备Id:")
		</td>
		<td>
            @Html.TextBoxFor(model => model.MachineId,new{@id="e_MachineId"})
		</td>
	</tr>

	<tr>
		<td>
            @Html.LabelFor(model => model.StatusOld,"状态:")
		</td>
		<td>
            @Html.TextBoxFor(model => model.StatusOld,new{@id="e_StatusOld"})
		</td>
	</tr>

	<tr>
		<td>
            @Html.LabelFor(model => model.UserNameOld,"使用者:")
		</td>
		<td>
            @Html.TextBoxFor(model => model.UserNameOld,new{@id="e_UserNameOld"})
		</td>
	</tr>

	<tr>
		<td>
            @Html.LabelFor(model => model.StartDateOld,"开始使用时间:")
		</td>
		<td>
            @Html.TextBoxFor(model => model.StartDateOld,new{@id="e_StartDateOld"})
		</td>
	</tr>

	<tr>
		<td>
            @Html.LabelFor(model => model.EndDateOld,"结束始用时间:")
		</td>
		<td>
            @Html.TextBoxFor(model => model.EndDateOld,new{@id="e_EndDateOld"})
		</td>
	</tr>


    
</table>
</fieldset>
}
