@model KeWeiOMS.Domain.AccountFeeType
@{
    Layout = null;
}

<script type="text/javascript">

    $(function () {


    });

</script>

@using (Html.BeginForm("Create", "AccountFee", FormMethod.Post, new { id = "c_form" }))
{
    <fieldset>
        <table class="editForm">
            <tr>
                <td>
                    @Html.LabelFor(model => model.Id, "主键标识:")
                </td>
                <td>
                    @Html.TextBoxFor(model => model.Id, new { @id = "c_Id" })
                </td>
            </tr>

            <tr>
                <td>
                    @Html.LabelFor(model => model.AccountId, "账户ID:")
                </td>
                <td>
                    @Html.TextBoxFor(model => model.AccountId, new { @id = "c_AccountId" })
                </td>
            </tr>

            <tr>
                <td>
                    @Html.LabelFor(model => model.AccountName, "账户名称:")
                </td>
                <td>
                    @Html.TextBoxFor(model => model.AccountName, new { @id = "c_AccountName" })
                </td>
            </tr>

            <tr>
                <td>
                    @Html.LabelFor(model => model.AmountBegin, "总金额开始:")
                </td>
                <td>
                    @Html.TextBoxFor(model => model.AmountBegin, new { @id = "c_AmountBegin" })
                </td>
            </tr>

            <tr>
                <td>
                    @Html.LabelFor(model => model.AmountEnd, "总金额结束:")
                </td>
                <td>
                    @Html.TextBoxFor(model => model.AmountEnd, new { @id = "c_AmountEnd" })
                </td>
            </tr>

            <tr>
                <td>
                    @Html.LabelFor(model => model.FeeFormula, "公式:")
                </td>
                <td>
                    @Html.TextBoxFor(model => model.FeeFormula, new { @id = "c_FeeFormula" })
                </td>
            </tr>

            <tr>
                <td>
                    @Html.LabelFor(model => model.FeeName, "费用名称:")
                </td>
                <td>
                    @Html.TextBoxFor(model => model.FeeName, new { @id = "c_FeeName" })
                </td>
            </tr>


        </table>
    </fieldset>
}
