@model KeWeiOMS.Domain.AccountFeeType

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>KeWeiOMS.Domain.AccountFeeType</title>
</head>
<body>
<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>KeWeiOMS.Domain.AccountFeeType</legend>
    <div class="display-label">主键标识</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Id)
    </div>

    <div class="display-label">账户ID</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.AccountId)
    </div>

    <div class="display-label">账户名称</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.AccountName)
    </div>

    <div class="display-label">总金额开始</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.AmountBegin)
    </div>

    <div class="display-label">总金额结束</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.AmountEnd)
    </div>

    <div class="display-label">公式</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.FeeFormula)
    </div>

    <div class="display-label">费用名称</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.FeeName)
    </div>


</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("返回列表", "Index")
    </p>
}


</body>
</html>
